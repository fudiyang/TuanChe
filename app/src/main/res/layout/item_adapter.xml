<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
  <RelativeLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      >
   <com.facebook.drawee.view.SimpleDraweeView
       android:id="@+id/img_simpleDraweeView"
       android:layout_width="50dp"
       android:layout_height="50dp"
       android:layout_margin="10dp"/>
   <TextView
       android:id="@+id/tv_11"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_margin="10dp"
       android:text="高团员"
       android:textSize="15dp"
       android:layout_toRightOf="@id/img_simpleDraweeView"
       />
      <RatingBar
          android:id="@+id/tv_12"
          style="@style/MyRatingBarsmall"
          android:layout_marginTop="4dip"
          android:layout_width="wrap_content"
          android:layout_height="20dp"
          android:numStars="5"
          android:layout_below="@id/tv_11"
          android:layout_toRightOf="@id/img_simpleDraweeView"
          android:isIndicator="true" />
      <TextView
          android:id="@+id/tv_13"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:text="2012-12-12"
          android:textSize="15dp"
          android:layout_toRightOf="@id/tv_12"
          android:layout_alignBottom="@id/tv_12"
          android:layout_marginLeft="15dp"
          />
  </RelativeLayout>
<TextView
    android:id="@+id/tv_14"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textSize="15dp"
    android:text="不错，还是可以"
    android:layout_marginLeft="15dp"/>


</LinearLayout>